[core]
    editor = nvim
    pager = delta
[init]
    defaultBranch = main  
[interactive]
    diffFilter = delta --color-only
[delta]
    navigate = true    # use n and N to move between diff sections
    dark = true
    side-by-side = true
[merge]
    conflictstyle = diff3
[diff]
    colorMoved = default
